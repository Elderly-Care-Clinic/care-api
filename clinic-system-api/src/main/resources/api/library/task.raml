#%RAML 1.0 Library
types:
  taskCreate:
    type: object
    properties:
      taskName:
        type: string
        example: "assignedTask"
      description:
        type: string
        example: "task assigned"
  
  taskUpdate:
    type: object
    properties:
      id:
        type: number
        example: 242424
      taskName?:
        type: string
        example: "assignedTask"
      description?:
        type: string
        example: "task assigned"
          
          